{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "TIT - That's it Team",
  "manifest_version": 3,
  "version": "1.0.0",
  "description": "Tiện ích cung cấp các tính năng hỗ trợ các bạn sinh viên trong quá trình sử dụng trang tín chỉ, tiện ích được sử dụng cho trang tín chỉ của trường Đại học Khoa Học Huế và Đại học Ngoại Ngữ Huế",
  "icons": {
    "128": "images/TIT-LOGO.png",
    "64": "images/TIT-LOGO.png",
    "32": "images/TIT-LOGO.png"
  },
  "background": {
    "service_worker": "scripts/background.js"
  },
  "minimum_chrome_version": "92",
  "action": {
    "default_title": "TIT - EXTENSION",
    "default_icon": "images/TIT-LOGO.png",
    "default_popup": "page/popup/index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://student.husc.edu.vn/*",
        "https://student.husc.edu.vn/*",
        "https://student.huflis.edu.vn/*"
      ],
      "css": [
        "css/lib/fa.css",
        "css/custom/component/modal_statistics.css",
        "css/custom/component/card_info.css",
        "css/custom/component/card_statistic.css",
        "css/custom/TIT/cursor.css",
        "css/custom/TIT/flexbox.css",
        "css/custom/TIT/margin.css",
        "css/custom/TIT/padding.css",
        "css/custom/TIT/text.css"
      ],
      "js": [
        "scripts/lib/xlsx.full.min.js",
        "scripts/lib/chart.js",
        "scripts/constants/route.js",
        "scripts/utils/route.js",
        "scripts/utils/DOM.js",
        "scripts/utils/string.js",
        "scripts/view/Layout/welcome.js",
        "scripts/view/Layout/notification.js",
        "scripts/view/Layout/dropdown.js",
        "scripts/view/CourseRegistration/index.js",
        "scripts/view/HistoryOfStudying/index.js"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "page/modal/course.html",
        "page/modal/statistics.html",
        "page/modal/new_modal.html"
      ],
      "matches": ["https://*/*"]
    }
  ],
  "permissions": ["storage", "notifications", "tabs", "activeTab"],
  "content_security_policy": {}
}
